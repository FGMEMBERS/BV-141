<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>throttle.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>base</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- RPM -->
  <model>
    <path>Aircraft/BV-141/Models/Interior/Panel/Instruments/rpm/rpm.xml</path>
    <offsets>
      <x-m> -0.292 </x-m>
      <y-m>  0.002 </y-m>
      <z-m>  0.030 </z-m>
      <pitch-deg>   -45 </pitch-deg> 
      <heading-deg>  45 </heading-deg>
    </offsets>
  </model>

  <!-- MAN -->
  <model>
    <path>Aircraft/BV-141/Models/Interior/Panel/Instruments/man/man.xml</path>
    <offsets>
      <x-m> -0.194 </x-m>
      <y-m> -0.017 </y-m>
      <z-m>  0.030 </z-m>
      <pitch-deg>   -45 </pitch-deg> 
      <heading-deg>  45 </heading-deg>
    </offsets>
  </model>

